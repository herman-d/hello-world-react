{
  "name": "hello-world-react",
  "description": "An OpenComponent that contains a React component",
  "version": "1.0.7",
  "oc": {
    "files": {
      "data": "server.js",
      "template": {
        "src": "app.js",
        "type": "oc-template-react"
      }
    },
    "parameters": {
      "name": {
        "default": "World",
        "description": "Your name",
        "example": "Jane Doe",
        "mandatory": false,
        "type": "string"
      }
    }
  },
  "devDependencies": {
    "oc-template-react-compiler": "5.2.0"
  }
}
